  "private": true,
  "homepage": "./",
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.61",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@ya.praktikum/react-developer-burger-ui-components": "^1.14.0",
    "axios": "^1.6.2",
    "immutability-helper": "^3.1.1",
    "punycode": "^2.3.1",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.5.3",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.0",
    "react-scripts": "5.0.1",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "typescript": "^4.9.5",
    "uuid": "^9.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --runInBand --detectOpenHandles --transformIgnorePatterns \"node_modules/(?!axios)/\" --watchAll",
    "test-deploy": "react-scripts test --runInBand --detectOpenHandles --transformIgnorePatterns \"node_modules/(?!axios)/\" --watchAll=false",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "cypress:open": "./node_modules/.bin/cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/uuid": "^9.0.7",
    "cypress": "^13.6.4",
    "eslint-plugin-cypress": "^2.15.1",
    "gh-pages": "^6.1.0"
  }
